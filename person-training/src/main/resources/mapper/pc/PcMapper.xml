<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itts.personTraining.mapper.PcMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.itts.personTraining.model.Pc">
        <id column="id" property="id" />
        <result column="pcmc" property="pcmc" />
        <result column="pch" property="pch" />
        <result column="pclx" property="pclx" />
        <result column="sczt" property="sczt" />
        <result column="pcnf" property="pcnf" />
        <result column="cjsj" property="cjsj" />
        <result column="cjr" property="cjr" />
        <result column="gxsj" property="gxsj" />
        <result column="gxr" property="gxr" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pcmc, pch, pclx, sczt, pcnf, cjsj, cjr, gxsj, gxr
    </sql>

</mapper>
